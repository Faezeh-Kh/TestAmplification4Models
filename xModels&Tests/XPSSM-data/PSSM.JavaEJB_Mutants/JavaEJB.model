      </transitions>
      <transitions
          name="ER2ENR"
          source="//@statemachine/@regions.0/@vertice.2"
          target="//@statemachine/@regions.0/@vertice.4"
          kind="external">
        <triggers
            name="releaseTrigger">
          <eventType
              xsi:type="statemachines:SignalEventType"
              signal="//@signals.7"/>
        </triggers>
      </transitions>
      <transitions
          name="ENR2ER"
          source="//@statemachine/@regions.0/@vertice.4"
          target="//@statemachine/@regions.0/@vertice.2"
          kind="external">
        <triggers
            name="handleTrigger">
          <eventType
              xsi:type="statemachines:SignalEventType"
              signal="//@signals.8"/>
        </triggers>
      </transitions>
      <transitions
          name="ENR2NENR"
          source="//@statemachine/@regions.0/@vertice.4"
          target="//@statemachine/@regions.0/@vertice.1"
          kind="external">
        <triggers
            name="timeoutTrigger">
          <eventType
              xsi:type="statemachines:SignalEventType"
              signal="//@signals.5"/>
        </triggers>
      </transitions>
      <transitions
          name="ENR2NENR"
          source="//@statemachine/@regions.0/@vertice.4"
          target="//@statemachine/@regions.0/@vertice.1"
          kind="external">
        <triggers
            name="crashTrigger">
          <eventType
              xsi:type="statemachines:SignalEventType"
              signal="//@signals.6"/>
        </triggers>
      </transitions>
      <transitions
          name="NER2NER"
          source="//@statemachine/@regions.0/@vertice.3"
