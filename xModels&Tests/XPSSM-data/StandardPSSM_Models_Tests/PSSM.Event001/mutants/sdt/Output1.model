<?xml version="1.0" encoding="ASCII"?>
<statemachines:CustomSystem xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:statemachines="http://.pssm.reactive/statemachines/" xsi:schemaLocation="http://.pssm.reactive/statemachines/ ../../../model/statemachines.ecore">
  <statemachine name="Event001_Test$behavior$1">
    <regions name="Region1">
      <vertice xsi:type="statemachines:Pseudostate" name="Initial1" outgoingTransitions="//@statemachine/@regions.0/@transitions.0"/>
      <vertice xsi:type="statemachines:State" name="wait" incomingTransitions="//@statemachine/@regions.0/@transitions.0 //@statemachine/@regions.0/@transitions.1">
        <exit name="wait(exit)"/>
      </vertice>
      <vertice xsi:type="statemachines:FinalState" name="FinalState1" outgoingTransitions="//@statemachine/@regions.0/@transitions.1"/>
      <transitions name="T1" source="//@statemachine/@regions.0/@vertice.0" target="//@statemachine/@regions.0/@vertice.1" kind="external"/>
      <transitions name="T2" source="//@statemachine/@regions.0/@vertice.2" target="//@statemachine/@regions.0/@vertice.1" kind="external">
        <triggers name="StartTrigger">
          <eventType xsi:type="statemachines:SignalEventType" signal="//@signals.0"/>
        </triggers>
        <effect name="T2(testEnd)"/>
      </transitions>
    </regions>
  </statemachine>
  <signals name="Start"/>
</statemachines:CustomSystem>
